    </bindings>
    <client>
      <endpoint address="http://54.173.24.177/painel/ServiceSms.asmx" binding="basicHttpBinding" bindingConfiguration="ServiceSMSSoap" contract="LOCASMS.ServiceSMSSoap" name="ServiceSMSSoap" />
